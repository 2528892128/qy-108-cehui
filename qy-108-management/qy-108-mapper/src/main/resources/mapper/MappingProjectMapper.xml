  <select id="selectAllProjectResult" resultMap="BaseResultMap" >
    SELECT a.id, a.project_type, a.project_name, a.project_leader, a.start_date, a.end_date,
      a.complete_time, a.acceptance_department, a.project_amount,
      a.results_status, a.user_id
    FROM t_mapping_project a
    JOIN t_result_commit b
    ON a.id=b.ref_id
  </select>
  <select id="selectAllProjectResultByType" resultMap="BaseResultMap" parameterType="java.lang.String" >
    SELECT a.id, a.project_type, a.project_name, a.project_leader, a.start_date, a.end_date,
      a.complete_time, a.acceptance_department, a.project_amount,
      a.results_status, a.user_id
    FROM t_mapping_project a
    JOIN t_result_commit b
    ON a.id=b.ref_id
    <where>
      1=1
      <if test="projectType != null and projectType != '' ">
        AND project_type=#{projectType, jdbcType=VARCHAR}
      </if>
    </where>
  </select>
  <select id="fuzzyProjectName" resultMap="BaseResultMap" parameterType="java.lang.String" >
    SELECT id, project_name, start_date
    FROM t_mapping_project
    <where>
      1=1
      <if test="projectName != null and projectName != '' ">
        AND project_name LIKE CONCAT('%', CONCAT(#{projectName, jdbcType=VARCHAR},'%'))
      </if>
      <if test="projectType != null and projectType != '' ">
        AND project_type=#{projectType, jdbcType=VARCHAR}
      </if>
      <if test="startDate != null and startDate != '' ">
        AND start_date=#{startDate, jdbcType=VARCHAR}
      </if>
    </where>
    ORDER BY id ASC
  </select>
